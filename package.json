  "private": true,
  "scripts": {
    "develop": "npm-run-all dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm-run-all lint:*",
    "lint:js": "next lint",
    "lint:css": "stylelint --fix './**/*.css' --ignore-path .gitignore",
    "fmt": "npm-run-all format",
    "format": "prettier --write . --ignore-path .gitignore",
