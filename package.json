    "react-leaflet": "^3.2.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-css-modules": "^4.0.1",
    "stylelint-config-prettier": "^9.0.3"
